  "version": "1.0.0",
  "dependencies": {
    "classnames": "^2.2.5",
    "howler": "^2.0.8",
    "localforage": "^1.5.6",
    "material-ui-icons-next": "1.0.0-beta.17-next",
    "material-ui-next": "1.0.0-beta.34",
    "react": "16.0.0",
    "react-dom": "16.0.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "react-scripts": "1.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
